    </style>

    <style name="TabLayoutStyle" parent="Base.Widget.Design.TabLayout">
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontPath">fonts/OpenSans-SemiBold.ttf</item>
        <item name="android:lineSpacingMultiplier">1.5</item>
        <item name="android:letterSpacing">0.05</item>
    </style>

    <style name="TabLayoutStyleBold" parent="TabLayoutStyle"></style>

    <style name="LeaderBoardTabLayoutStyle" parent="Base.Widget.Design.TabLayout">
