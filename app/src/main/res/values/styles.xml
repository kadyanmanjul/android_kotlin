    <style name="InterestChipStyle" parent="@style/Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor">@color/chip_borderd_selector</item>
        <item name="chipStrokeColor">@color/interest_chip_bg_color_selector</item>
        <item name="chipStrokeWidth">@dimen/_1sdp</item>
        <item name="android:textColor">@color/interest_chip_text_color_selector</item>
