<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:padding="@dimen/_8sdp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:padding="@dimen/_8sdp"
            android:text="@string/name"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintBottom_toBottomOf="@id/name_tv"
            app:layout_constraintEnd_toStartOf="@+id/name_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/name_tv" />

        <TextView
            android:id="@+id/name_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:padding="@dimen/_8sdp"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:padding="@dimen/_8sdp"
            android:text="@string/date_of_birth"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintBottom_toBottomOf="@id/dob_tv"
            app:layout_constraintEnd_toStartOf="@+id/dob_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/dob_tv" />

        <TextView
            android:id="@+id/dob_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:padding="@dimen/_8sdp"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/name_tv" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:padding="@dimen/_8sdp"
            android:text="@string/gender"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintBottom_toBottomOf="@id/gender_tv"
            app:layout_constraintEnd_toStartOf="@+id/gender_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/gender_tv" />

        <TextView
            android:id="@+id/gender_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:padding="@dimen/_8sdp"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/dob_tv" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:padding="@dimen/_8sdp"
            android:text="@string/login_type"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintBottom_toBottomOf="@id/login_type_tv"
            app:layout_constraintEnd_toStartOf="@+id/login_type_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/login_type_tv" />

        <TextView
            android:id="@+id/login_type_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:padding="@dimen/_8sdp"
            android:textAppearance="@style/TextAppearance.JoshTypography.Body_Text_Normal_Regular"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/gender_tv" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>